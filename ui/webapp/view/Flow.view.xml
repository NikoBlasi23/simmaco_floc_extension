<mvc:View
    height="100%"
    controllerName="my.company.simmflocext.controller.Flow"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:smartField="sap.ui.comp.smartfield"
    xmlns:form="sap.ui.comp.smartform"
>
    <Page
        id="page"
        showHeader="true"
    >
        <customHeader>
            <Bar>
                <contentRight>
                    <Button
                        id="edit"
                        text="Edit"
                        enabled="true"
                        press="handleEditPress"
                    />
                    <Button
                        id="save"
                        text="Save"
                        type="Emphasized"
                        visible="false"
                        press="handleSavePress"
                    />
                    <Button
                        id="cancel"
                        text="Cancel"
                        visible="false"
                        press="handleCancelPress"
                    />
                </contentRight>
            </Bar>

        </customHeader>
       
              
                
                       
                  
           <content>
             <ScrollContainer
                    height="99%"
                    vertical="true"
                    horizontal="true"
                    id="DIOPORCO30"
                >
           </ScrollContainer>
           </content>
    </Page>
</mvc:View>
